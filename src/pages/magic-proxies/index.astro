---
import { getCollection } from "astro:content";
import PostsListLayout from "src/layouts/PostsListLayout.astro";

const allPosts = await getCollection(
	"magicProxies",
	(entry) => process.env.NODE_ENV === "development" || !entry.data.draft,
);
---

<PostsListLayout
	postsList={allPosts}
	listTitle="Magic Proxies"
	postDirectory="magic-proxies"
/>
