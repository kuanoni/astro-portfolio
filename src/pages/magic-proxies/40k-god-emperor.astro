---
import { Image } from "astro:assets";
import ImagePreview from "@components/blocks/ImagePreview.astro";
import GalleryLayout from "src/layouts/GalleryLayout.astro";
import dynamicImage from "@utils/dynamicImage";

const imgRecords = import.meta.glob<{ default: ImageMetadata }>(
    `src/assets/images/magic-proxies/40k-god-emperor/*.{jpeg,jpg,png,gif,webp}`,
);

const images = Object.keys(imgRecords).map((key) => {
    return dynamicImage(key);
});
---

<GalleryLayout galleryTitle="Warhammer 40K: God Emperor">
    <!-- {images.map((img) => <Image src={img} alt={""} layout="fixed" />)} -->
</GalleryLayout>
