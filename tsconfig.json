{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": [
				"src/components/*"
			],
			"@content/*": [
				"src/content/*"
			],
			"@images/*": [
				"src/assets/images/*"
			],
			"@pages/*": [
				"src/pages/*"
			],
			"@styles/*": [
				"src/styles/*"
			],
			"@utils/*": [
				"src/utils/*"
			]
		}
	}
}